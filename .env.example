# AI API Keys - 必須設定
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_TEXT_MODEL=gemini-2.5-flash
GEMINI_IMAGE_MODEL=gemini-2.5-flash-image

# Vercel KV (Upstash Redis) - 画像履歴保存用（オプション）
KV_REST_API_URL=https://your-kv-url.upstash.io
KV_REST_API_TOKEN=your-kv-token-here

# Environment Configuration
NODE_ENV=production

# CORS Configuration 
# 開発環境: 空にする（全てのoriginを許可）
# 本番環境: 許可するドメインをカンマ区切りで指定
CORS_ORIGIN=https://hp-ai-image-generator.vercel.app

# Logging Configuration
LOG_LEVEL=error

# API Configuration
RATE_LIMIT_MS=500
API_TIMEOUT_MS=120000
MAX_BATCH_SIZE=8
MAX_ANALYZE_PAGES=10

# Frontend Configuration (Vite用)
VITE_LOG_LEVEL=error